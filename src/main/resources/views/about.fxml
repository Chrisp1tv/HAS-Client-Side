<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" styleClass="root-container,about-view"
            xmlns="http://javafx.com/javafx/8.0.112-ea" fx:controller="chv.has.controllers.ConfigurationController">
    <center>
        <VBox styleClass="about-view-box">
            <ImageView>
                <Image url="/icons/icon.png"/>
            </ImageView>
            <Text text="%HAS" styleClass="about-title"/>
            <Text text="%developmentInformation" styleClass="about-information"/>
        </VBox>
    </center>
</BorderPane>